{
  "files.associations": {
    "partykit.json": "jsonc"
  },
  "json.schemas": [
    {
      "fileMatch": ["partykit.json"],
      "url": "https://www.partykit.io/schema.json"
    }
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  }
}
